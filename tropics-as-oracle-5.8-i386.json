{
  "builders": [
    {
      "boot_command": [
        "<tab> text ks=http://{{ .HTTPIP }}:{{ .HTTPPort }}/centos-5.8/ks.cfg<enter><wait>"
      ],
      "boot_wait": "10s",
      "disk_size": 40960,
      "guest_additions_path": "VBoxGuestAdditions_{{.Version}}.iso",
      "guest_os_type": "Oracle",
      "hard_drive_interface": "sata",
      "http_directory": "http",
      "iso_checksum": "e9f58baa2fc3cbd55e9b484d50071a9d",
      "iso_checksum_type": "md5",
      "iso_url": "{{user `iso_directory`}}/OracleLinux-R5-U8-Server-i386-dvd.iso",
      "output_directory": "tropics-as-oracle-5.8-i386-virtualbox",
      "shutdown_command": "echo 'vagrant'|sudo -S /sbin/halt -h -p",
      "ssh_password": "vagrant",
      "ssh_port": 22,
      "ssh_username": "vagrant",
      "ssh_wait_timeout": "10000s",
      "type": "virtualbox-iso",
      "vboxmanage": [
        [
          "modifyvm",
          "{{.Name}}",
          "--memory",
          "2048"
        ],
        [
          "modifyvm",
          "{{.Name}}",
          "--cpus",
          "1"
        ]
      ],
      "virtualbox_version_file": ".vbox_version",
      "vm_name": "tropics-as-oracle-5.8-i386"
    }
  ],
  "post-processors": [
    {
      "output": "builds/{{user `box_basename`}}.{{.Provider}}.box",
      "type": "vagrant"
    }
  ],
  "provisioners": [
    {
      "destination": "/tmp/bento-metadata.json",
      "source": "{{user `metadata`}}",
      "type": "file"
    },
    {
      "environment_vars": [],
      "execute_command": "echo 'vagrant' | {{.Vars}} sudo -S -E bash '{{.Path}}'",
      "scripts": [
        "scripts/common/metadata.sh",
        "scripts/common/vagrant.sh",
        "scripts/common/sshd.sh",
        "scripts/common/vmtools.sh",
		"scripts/tropics-as/oas-pre-install.sh",
        "scripts/tropics-as/cleanup.sh",
        "scripts/common/minimize.sh"
      ],
      "type": "shell"
    }
  ],
  "variables": {
    "arch": "32",
    "box_basename": "tropics-as-oracle-5.8-i386",
    "build_timestamp": "{{isotime \"20060102150405\"}}",
    "git_revision": "__unknown_git_revision__",
    "metadata": "floppy/dummy_metadata.json",
    "mirror": "http://mirrors.dotsrc.org/oracle-linux/EL5/U11/i386",
	"iso_directory": "iso",
	"software_directory": "software/tropics-as"
    "name": "tropics-as-oracle-5.8-i386",
    "template": "tropics-as-oracle-5.8-i386",
    "version": "2.0.TIMESTAMP"
  }
}